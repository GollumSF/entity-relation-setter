    "keywords" : [
        "setter",
        "entity",
        "relation",
        "gollumsf"
    ],
    "homepage": "https://github.com/GollumSF/eentity-relation-setter",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Damien Duboeuf",
            "email": "smeagolworms4@gmail.com"
        }
    ],
    "support" : {
        "email": "smeagolworms4@gmail.com",
        "wiki": "https://github.com/GollumSF/entity-relation-setter/blob/master/README.md"
    },
    "minimum-stability": "dev",
    "require" : {
        "php" : ">=7.1",
        "doctrine/orm": "^2,>=2.4.5"
    },
    "autoload" : {
        "psr-4" : {
            "GollumSF\\EntityRelationSetter\\" : "src/"
        }
